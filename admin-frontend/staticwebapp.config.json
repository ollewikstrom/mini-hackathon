{
	"routes": [
		{
			"route": "/*",
			"serve": "/index.html",
			"statusCode": 200
		}
	],
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/assets/*"]
	},
	"platform": {
		"apiRuntime": "node:18"
	},
	"mimeTypes": {
		".json": "text/json"
	}
}
